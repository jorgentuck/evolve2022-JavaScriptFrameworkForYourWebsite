<!-- DO NOT EDIT THIS FILE IN THE WYSIWYG -->
<variable section="RSS Setup (Not all displayed on web pages, but all are required for a valid RSS Feed)" name="rss-description" type="text" rows="5" prompt="RSS Description" alt="Give a short description for this RSS Item."></variable>
<variable name="rss-author" type="text" prompt="RSS Author" alt="Type in the author for this RSS Item. Format: email@domain.edu (FirstName LastName)"></variable>

<variable name="murl" type="filechooser" prompt="Media Image" alt="Select the Media Image for the RSS Item."></variable>
<variable name="mtitle" prompt="Media Image Title" alt="Enter a title for the Media Image. Image title is REQUIRED whenever an image is selected."></variable>
<variable name="mdesc" type="text" rows="2" prompt="Media Image Description" alt="Enter a description for the Media Image."></variable>
<variable name="mthumbnail" type="filechooser" prompt="Media Thumbnail" alt="Select a thumbnail version of the Media Image."></variable>
<variable name="mkeywords" prompt="Media Image Keywords" alt="Enter keywords that describe the Media Image."></variable>
